{{!-- Create a table --}}
<table>

    {{!-- Header section --}}
    <thead>

        {{!-- For just the first row, we print each key of the row object as a header cell so we
        know what each column means when the page renders --}}
        <tr>
            {{#each data.[0]}}
            <th>
                {{@key}}
            </th>
            {{/each}}
        </tr>
    </thead>

    {{!-- Body section --}}
    <tbody>

        {{!-- For each row, print the id, fname, lname, homeworld and age, in order --}}
        {{#each data}}
        <tr>
            <td>{{this.customerID}}</td>
            <td>{{this.customerFirstName}}</td>
            <td>{{this.customerLastName}}</td>
            <td>{{this.customerEmail}}</td>
            <td>{{this.customerStreetAddress}}</td>
            <td>{{this.customerCity}}</td>
            <td>{{this.customerZipcode}}</td>
            <td>{{this.customerState}}</td>
            <td>{{this.customerPhone}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>